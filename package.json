{
  "name": "message-api",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "clean": "rm -rfv package-lock.json node_modules .angular dist src/message-api && npm cache clean --force",
    "generate": "java -jar util/openapi-generator-cli.jar validate -i src/message-api.json && java -jar util/openapi-generator-cli.jar generate -i src/message-api.json -g typescript-angular -o src/message-api",
    "build": "ng build message-api",
    "pack": "npm pack ./dist/message-api --pack-destination dist"
  },
  "engines": {
    "node": ">=22.12.0 <23",
    "npm": ">=10.9.0 <11"
  },
  "dependencies": {
    "tslib": "~2.6.2"
  },
  "peerDependencies": {
    "@angular/core": "~19.2.14",
    "@angular/common": "~19.2.14",
    "@angular/compiler": "~19.2.14",
    "@angular/forms": "~19.2.14",
    "@angular/platform-browser": "~19.2.14",
    "@angular/platform-browser-dynamic": "~19.2.14",
    "rxjs": "~7.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/cli": "~19.2.14",
    "@angular/build": "~19.2.14",
    "@angular/compiler-cli": "~19.2.14",
    "@angular-devkit/build-angular": "~19.2.14",
    "ng-packagr": "~19.2.2",
    "typescript": "~5.8.3"
  }
}
